                  .order_row
                    .order_td.order_td-first
                      label.order_label(for="o_kpp") КПП:
                    .order_td
                      input#o_kpp.order_input(type="text", name="o_kpp", readonly)
                  //2
                  .order_row
                    .order_td.order_td-first
                      label.order_label(for="o_inn") ИНН:
                    .order_td
                      input#o_inn.order_input(type="text", name="o_inn", readonly)
                  //3
