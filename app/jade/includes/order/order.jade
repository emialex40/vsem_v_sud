                    .order_td.order_td-first
                      label.order_label(for="o_fio") ФИО плательщика* :
                    .order_td
                      input#o_fio.order_input(type="text", name="o_fio", required)
                  //10
                  .order_row
                    .order_td.order_td-first
                      label.order_label(for="o_adress") Адрес Плательшика* :
                    .order_td
                      input#o_adress.order_input(type="text", name="o_adress", required)
                  //11
                  .order_row
                    .order_td.order_td-first
                      label.order_label(for="o_date") Дата:
                    .order_td
                      input#o_date.order_input(type="text", name="o_date")
                  //12
                  .order_row
                    .order_td.order_td-first
                      label.order_label(for="o_sum") Сумма:
                    .order_td
                      input#o_sum.order_input(type="text", name="o_sum")
                  .order_row
                    .order_td.order_td-first
                      input.order_submit(type="submit", name="Рассчитать", id="_calc", value="Скачать заполненную квитанцию")
                    .order_td
